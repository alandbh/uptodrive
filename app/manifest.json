{
    "name": "UP To Drive",
    "short_name": "Up to Drive 2",
    "description": "A humble file uploader",
    "lang": "en-US",
    "Ã¯d": "/?homescreen=2",
    "icons": [
        {
            "src": "/icons/icon-512x512.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "maskable"
        },
        {
            "src": "/icons/android-chrome-192x192.png",
            "sizes": "192x192",
            "type": "image/png",
            "purpose": "any"
        },
        {
            "src": "/icons/icon-512x512.png",
            "sizes": "512x512",
            "type": "image/png"
        }
    ],
    "share_target": {
        "action": "/upload",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
            "title": "title",
            "text": "text",
            "files": [
                {
                    "name": "file",
                    "accept": [
                        "image/*",
                        "video/*",
                        "audio/*",
                        "application/pdf"
                    ]
                }
            ]
        }
    },
    "theme_color": "#FF0000",
    "background_color": "#FF0000",
    "start_url": "/",
    "scope": "./",
    "display": "standalone",
    "orientation": "portrait",
    "launch_handler": {
        "client_mode": "auto"
    }
}
